{
  "name": "vuepress_blog",
  "version": "1.0.0",
  "description": "vuePress 风格的文档",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "type": "module",
  "scripts": {
    "dev": "vuepress dev docs --clean-cache",
    "build:github": "cross-env NODE_ENV=github vuepress build docs --clean-cache",
    "build:jdy": "cross-env NODE_ENV=jdy vuepress build docs --clean-cache",
    "commit": "git add --all && git commit -m 'add' && git push",
    "node:commit": "node commit"
  },
  "author": "lizonglin",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@vuepress/cli": "^2.0.0-beta.67",
    "vuepress": "^2.0.0-beta.67",
    "vuepress-plugin-copy-code2": "^2.0.0-alpha.38",
    "vuepress-plugin-full-text-search2": "^0.4.0"
  },
  "dependencies": {
    "@vuepress/plugin-register-components": "2.0.0-beta.38"
  }
}
